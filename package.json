{
  "name": "qwik-playable",
  "description": "Starter with built-in routing, powered by Qwik City Vite.js tooling. Netlify Edge Functions. Prettier code formatter. Tailwind CSS framework.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.netlify.ts",
    "dev": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --mode ssr",
    "dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "serve": "netlify dev",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.0.30",
    "@builder.io/qwik-city": "0.0.13",
    "@netlify/vite-plugin-netlify-edge": "^1.1.0",
    "@types/eslint": "8.4.2",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.16.0",
    "eslint-plugin-qwik": "0.0.30",
    "node-fetch": "2.6.7",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.24",
    "typescript": "4.7.2",
    "vite": "2.9.9"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true
}
